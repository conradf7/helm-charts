{
  "cluster_key": "{{ .Values.data.name }}-to-{{ .Values.data.deploy.jwt_name }}-cluster",
  "zone_key": "zone-default-zone",
  "name": "{{ .Values.data.deploy.jwt_name }}",
  "require_tls": true,
  "secret": {
    "secret_key": "secret-{{ .Values.data.deploy.jwt_name }}-secret",
    "secret_name": "spiffe://{{ .Values.global.spire.trust_domain}}/{{ .Values.data.name }}",
    "secret_validation_name": "spiffe://{{ .Values.global.spire.trust_domain}}",
    "ecdh_curves": [
      "X25519:P-256:P-521:P-384"
    ],
    "subject_names": [
      "spiffe://{{ .Values.global.spire.trust_domain}}/{{ .Values.data.deploy.jwt_name }}"
    ]
  },
  "circuit_breakers": null,
  "outlier_detection": null,
  "health_checks": []
}
