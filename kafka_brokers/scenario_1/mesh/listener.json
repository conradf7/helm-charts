{
    "zone_key": "zone-default-zone",
    "listener_key": "fibonacci-listener",
    "domain_keys": [
        "fibonacci-domain"
    ],
    "name": "ingress",
    "ip": "0.0.0.0",
    "port": 10808,
    "protocol": "http_auto",
    "active_http_filters": [
        "gm.observables"
      ],
    "http_filters": {
        "gm_observables": {
            "useKafka": true,
            "topic": "fibonacci",
            "logLevel": "debug",
            "eventTopic": "fibonacci-test",
            "kafkaServerConnection": "kafka-broker0.observables.svc.cluster.local:9092,kafka-broker1.observables.svc.cluster.local:9092,kafka-broker2.observables.svc.cluster.local:9092"
          }
      },
    "secret": {
        "secret_key": "fibonacci.identity",
        "secret_name": "spiffe://quickstart.greymatter.io/fibonacci",
        "secret_validation_name": "spiffe://quickstart.greymatter.io",
        "subject_names": [
            "spiffe://quickstart.greymatter.io/edge"
        ],
        "ecdh_curves": [
            "X25519:P-256:P-521:P-384"
        ]
    }
}